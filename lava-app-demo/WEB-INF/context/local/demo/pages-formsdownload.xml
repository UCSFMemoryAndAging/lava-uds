<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  <bean id="ucdlavaFormsDownloadFlowTypeBuilder" class="edu.ucsf.lava.crms.webflow.builder.FormsDownloadFlowTypeBuilder"/>

  <bean id="formsdownloadFormAction" parent="crmsEntityComponentFormAction">
    <constructor-arg>
      <list>
        <bean class="edu.ucsf.lava.crms.mylava.controller.FormsDownloadHandler" parent="crmsHandler"/>
      </list>
    </constructor-arg>
  </bean>

  <bean id="formsDownloadFlowAction" abstract="true" parent="crmsAction">
    <property name="flowTypeBuilder">
      <ref bean="ucdlavaFormsDownloadFlowTypeBuilder"/>
    </property>
    <property name="patientSectionDefault" value="true"/>
    <property name="patientModuleDefault" value="true"/>
    <property name="sectionDefault" value="true"/>
    <property name="moduleDefault" value="true"/>
    <property name="homeDefault" value="true"/>
  </bean>
  <bean id="lava.crms.mylava.formsdownload.formsdownload" parent="formsDownloadFlowAction"/>

</beans>
