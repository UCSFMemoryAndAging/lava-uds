<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  <bean id="list.udsclinicianassessment3.codes" parent="list.uds.codes"/>
  <bean id="lava.crms.assessment.instrument.udsclinicianassessment3" parent="crmsInstrumentFlowAction"/>
  <bean id="udsclinicianassessment3FormAction" parent="udsInstrumentFormAction"/>
  <bean id="udsclinicianassessment3" parent="instrumentConfig">
    <property name="className"><value>edu.ucsf.lava.crms.assessment.model.UDSClinicianAssessment3</value></property>
    <property name="supportedFlows"><value>enter</value></property>
    <property name="currentVersionAlias"><value>udsclinicianassessment3</value></property>
  </bean>


  <bean id="list.uds3.udsclinicianassessment3.CANCER" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="No"/>
        <constructor-arg value="0"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes, primary/non-metastatic"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes, metastatic"/>
        <constructor-arg value="2"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Not assessed"/>
        <constructor-arg value="8"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.udsclinicianassessment3.DIABET" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="No"/>
        <constructor-arg value="0"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes, Type I"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes, Type II"/>
        <constructor-arg value="2"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes, other type"/>
        <constructor-arg value="3"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Not assessed or unknown"/>
        <constructor-arg value="9"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.udsclinicianassessment3.ARTYPE" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="Rheumatoid"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Osteoarthritis"/>
        <constructor-arg value="2"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Other (specify)"/>
        <constructor-arg value="3"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Unknown"/>
        <constructor-arg value="9"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.udsclinicianassessment3.ARTUPEX" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="No"/>
        <constructor-arg value="0"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes"/>
        <constructor-arg value="1"/>
      </bean> 
    </list></property> 
  </bean>



</beans>

