    <property name="defaultMaxValue" value="87"/>
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="Unknown"/>
        <constructor-arg value="99"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.udshealthhistory3.PACKSPER" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="1 cigarette to less than 1/2 pack"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="1/2 pack to less than 1 pack"/>
        <constructor-arg value="2"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="1 pack to 1 1/2 packs"/>
