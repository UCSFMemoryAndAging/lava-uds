<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="list.uds3.common.noyes01" parent="list.standardUds">
      <property name="data"><list>
        <bean parent="list.data">
            <constructor-arg value="No"/>
            <constructor-arg value="0"/>
        </bean>
        <bean parent="list.data">
            <constructor-arg value="Yes"/>
            <constructor-arg value="1"/>
        </bean>
      </list></property>
    </bean>
  <bean id="list.uds3.common.year" parent="list.numericRangeUds"> 
    <property name="defaultMinValue" value="1875"/>
    <property name="defaultMaxValue" value="2015"/>
  </bean>

  <bean id="list.uds3.common.whyunable" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="Physical problem"/>
        <constructor-arg value="95"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Cognitive/behavior problem"/>
        <constructor-arg value="96"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Other problem"/>
        <constructor-arg value="97"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Verbal refusal"/>
        <constructor-arg value="98"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.common.gender" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="Male"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Female"/>
        <constructor-arg value="2"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.common.noyesunknown" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="No"/>
        <constructor-arg value="0"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Unknown"/>
        <constructor-arg value="9"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.common.range0_15" parent="list.numericRangeUds"> 
    <property name="defaultMinValue" value="0"/>
    <property name="defaultMaxValue" value="15"/>
  </bean>
  <bean id="list.uds3.common.month" parent="list.numericRangeUds"> 
    <property name="defaultMinValue" value="1"/>
    <property name="defaultMaxValue" value="12"/>
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="Unknown"/>
        <constructor-arg value="99"/>
      </bean> 
    </list></property> 
  </bean>
  <bean id="list.uds3.common.NoYesNa" parent="list.numericRangeUds"> 
    <property name="data"><list> 
      <bean parent="list.data">
        <constructor-arg value="No"/>
        <constructor-arg value="0"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Yes"/>
        <constructor-arg value="1"/>
      </bean> 
      <bean parent="list.data">
        <constructor-arg value="Not assessed"/>
        <constructor-arg value="8"/>
      </bean> 
    </list></property> 
  </bean>

</beans>
