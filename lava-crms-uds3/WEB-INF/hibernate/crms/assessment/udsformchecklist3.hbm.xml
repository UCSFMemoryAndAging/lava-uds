            <property name="a2not" column="A2NOT" type="int" />
            <property name="a2comm" column="A2COMM" type="string" length="60"/>
            <property name="a3sub" column="A3SUB" type="int" />
            <property name="a3not" column="A3NOT" type="int" />
            <property name="a3comm" column="A3COMM" type="string" length="60"/>
