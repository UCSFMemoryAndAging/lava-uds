            <property name="a4not" column="A4NOT" type="int" />
            <property name="a4comm" column="A4COMM" type="string" length="60"/>
            <property name="b1sub" column="B1SUB" type="int" />
            <property name="b1not" column="B1NOT" type="int" />
            <property name="b1comm" column="B1COMM" type="string" length="60"/>
            <property name="b5sub" column="B5SUB" type="int" />
            <property name="b5not" column="B5NOT" type="int" />
            <property name="b5comm" column="B5COMM" type="string" length="60"/>
            <property name="b6sub" column="B6SUB" type="int" />
