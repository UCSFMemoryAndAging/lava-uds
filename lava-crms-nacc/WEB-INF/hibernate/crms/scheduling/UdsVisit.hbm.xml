<?xml version="1.0"?>
    
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<joined-subclass name="edu.ucsf.lava.crms.scheduling.model.UdsVisit" table="udsvisit" extends="edu.ucsf.lava.crms.scheduling.model.Visit" select-before-update="true">

	   	<key column="VID"/>
	   
		<property name="packet" column="Packet" type="string" length="2"/>
		<property name="visitNum" column="VisitNum" type="short"/>
			
    
</joined-subclass>
	
	
</hibernate-mapping>

